{
  "hosting": {
    "public": "public",
    "site": "raedam-front-facing-website",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
         "source": "**",
         "function": "frontFacingNodeServer"      
      }
    ]
  },
  "functions": {
    "engines": {"node": "10"},
    "runtime": "nodejs10"
  }
}
